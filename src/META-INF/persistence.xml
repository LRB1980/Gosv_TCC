<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Gosv" transaction-type="JTA">
		<jta-data-source>java:jboss/datasources/MysqlDS</jta-data-source>

		
		<class>br.com.gosv.model.Agendamento</class>
		<class>br.com.gosv.model.Cliente</class>
		<class>br.com.gosv.model.Endereco</class>
		<class>br.com.gosv.model.Funcionario</class>
		<class>br.com.gosv.model.Localizacao</class>
		<class>br.com.gosv.model.OrdemServico</class>
		<class>br.com.gosv.model.Servico</class>
		<class>br.com.gosv.model.Setor</class>
		<class>br.com.gosv.model.StatusServico</class>
		<class>br.com.gosv.model.StatusVisita</class>
		<class>br.com.gosv.model.Usuario</class>
		<class>br.com.gosv.model.Visita</class>
		<properties>
			<property name="eclipselink.logging.level" value="ALL"/>
		</properties>
	</persistence-unit>
</persistence>
